{
  "name": "datavisyn-scatterplot-react",
  "version": "0.0.1",
  "description": "a canvas based scatterplot implementation",
  "homepage": "https://phovea.caleydo.org",
  "author": {
    "name": "datavisyn GmbH",
    "email": "contact@datavisyn.io",
    "url": "https://datavisyn.io"
  },
  "contributors": [
    {
      "name": "Samuel Gratzl",
      "email": "samuel.gratzl@datavisyn.io"
    }
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/datavisyn/datavisyn-scatterplot-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datavisyn/datavisyn-scatterplot-react.git"
  },
  "main": "build/scatterplot.js",
  "typings": "typings/index.d.ts",
  "files": [
    "src",
    "build",
    "typings"
  ],
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json src/**.ts test/**.ts",
    "docs": "typedoc --options typedoc.json src/**.ts",
    "prebuild": "npm run test",
    "build": "webpack -p --env prod",
    "build:dev": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --inline --hot",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "pretest": "npm run compile",
    "test": "karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "predist": "npm run build",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/datavisyn-scatterplot-react.tar.gz *"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^3.2.0",
    "css-loader": "^0.25.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.0",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typedoc": "^0.5.0",
    "typescript": "2.1.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "dependencies": {
    "@types/d3-brush": "^1.0.6",
    "@types/react": "^0.14.42",
    "@types/react-dom": "^0.14.18",
    "d3-brush": "^1.0.3",
    "datavisyn-scatterplot": "github:datavisyn/datavisyn-scatterplot",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
