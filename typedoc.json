{
  "mode": "modules",
  "out": "./build/docs",
  "theme": "minimal",
  "name": "LineUp.js",
  "module": "commonjs",
  "media": "media",
  "ignoreCompilerErrors": "true",
  "experimentalDecorators": "true",
  "emitDecoratorMetadata": "true",
  "target": "ES6",
  "moduleResolution": "node",
  "preserveConstEnums": "true",
  "stripInternal": "true",
  "suppressExcessPropertyErrors": "true",
  "suppressImplicitAnyIndexErrors": "true"
}
